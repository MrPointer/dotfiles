#!/bin/sh
#shellcheck disable=SC1000-SC9999

SEDG_DIR="$HOME/.sedg"

SEDG_EXECUTABLES_DIR="$SEDG_DIR/bin"
SEDG_FIX_VPN_EXECUTABLE_NAME="fix-vpn"
SEDG_FIX_VPN_EXECUTABLE_PATH="${SEDG_EXECUTABLES_DIR}/${SEDG_FIX_VPN_EXECUTABLE_NAME}"

SEDG_ZSH_DIR="$SEDG_DIR/zsh"
SEDG_ZSH_RC_EXTENSION="$SEDG_ZSH_DIR/.zshrc"

SEDG_IOE_DEV_ENV_FRAMEWORK_DIR="$HOME/dev-env"
SEDG_IOE_DEV_ENV_FRAMEWORK_ENTRY_POINT="$HOME/dev-env.sh"
